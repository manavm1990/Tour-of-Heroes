{
  "better-comments.highlightPlainText": true,
  "editor.codeActionsOnSave": {
    "addMissingImports": true,
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "editor.autoIndent": "full",
  "editor.codeLens": false,
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorStyle": "line",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "'Victor Mono', 'Inconsolata', 'Source Code Pro', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "editor.letterSpacing": 0.5,
  "editor.lineHeight": 25,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.scrollBeyondLastColumn": 2,
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 80,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "files.defaultLanguage": "",
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "javascript.validate.enable": false,
  "prettier.requireConfig": true,
  "stylelint.enable": true,
  "terminal.integrated.fontFamily": "'Inconsolata', 'Source Code Pro', monospace",
  "workbench.colorTheme": "Winter is Coming (Dark Black - No Italics)",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.view.alwaysShowHeaderActions": true
}
